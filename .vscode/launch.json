    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) cpu",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/cpu/cpu",
            "args": ["${workspaceFolder}/cpu/cfg/cpu_integral_1.config"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Habilitar la impresión con sangría para gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
        ]
    },
